<?xml version="1.0" encoding="UTF-8"?>
<data xmlns="http://www.woltlab.com" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.woltlab.com http://www.woltlab.com/XSD/options.xsd">
	<import>
		<!-- Max participants option -->
		<option>
			<name>calendar_advanced_import_max_participants</name>
			<categoryname>calendar.import</categoryname>
			<optiontype>integer</optiontype>
			<defaultvalue>0</defaultvalue>
			<validationpattern>^\d+$</validationpattern>
			<enableoptions>calendar_advanced_import_max_participants_help</enableoptions>
		</option>

		<!-- Public participants list option -->
		<option>
			<name>calendar_advanced_import_public_participants_list</name>
			<categoryname>calendar.import</categoryname>
			<optiontype>boolean</optiontype>
			<defaultvalue>1</defaultvalue>
		</option>

		<!-- Auto signup deadline option -->
		<option>
			<name>calendar_advanced_import_auto_signup_deadline</name>
			<categoryname>calendar.import</categoryname>
			<optiontype>boolean</optiontype>
			<defaultvalue>1</defaultvalue>
		</option>

		<!-- Enable signups option -->
		<option>
			<name>calendar_advanced_import_enable_signups</name>
			<categoryname>calendar.import</categoryname>
			<optiontype>boolean</optiontype>
			<defaultvalue>1</defaultvalue>
		</option>
	</import>
</data>